class Main {
  public static void main(String[] args) {

    // GCD between n1 and n2
    int a1 = 72, a2 = 234;
    
    // initially set to gcd
    int gcd = 1;

    for (int i = 1; i <= a1 && i <= a2; ++i) {

      // check if i perfectly divides both n1 and n2
      if (a1 % i == 0 && a2 % i == 0)
        gcd = i;
    }

    System.out.println("GCD of " + a1 +" and " + a2 + " is " + gcd);
  }
}
